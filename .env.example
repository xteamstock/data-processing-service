# Environment variables for Data Processing Service

# Google Cloud Project
GOOGLE_CLOUD_PROJECT=competitor-destroyer

# Pub/Sub topic prefix
PUBSUB_TOPIC_PREFIX=social-analytics

# Media tracking (optional)
MEDIA_TRACKING_ENABLED=true

# Media storage bucket
MEDIA_STORAGE_BUCKET=social-analytics-media

# BigQuery dataset
BIGQUERY_DATASET_ID=social_analytics

# GCS buckets
RAW_DATA_BUCKET=social-analytics-raw-data
PROCESSED_DATA_BUCKET=social-analytics-processed-data